{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Disable Command-Tab application switcher",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "tab",
                                    "modifiers": {
                                        "mandatory": ["left_command"],
                                        "optional": ["shift"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "n",
                                        "modifiers": ["command", "control", "option"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Map Cmd-f to Ctrl-f in Ghostty for tmux prefix",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": ["com.mitchellh.ghostty"],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": {
                                    "key_code": "f",
                                    "modifiers": {
                                        "mandatory": ["left_command"]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "f",
                                        "modifiers": ["left_control"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Rebind Command-H to Hyper-H",
                        "enabled": false,
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": { "mandatory": ["left_command"] }
                                },
                                "to": [
                                    {
                                        "key_code": "h",
                                        "modifiers": ["command", "control", "option"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
            "name": "Default profile",
            "selected": true,
            "simple_modifications": [
                {
                    "from": { "key_code": "caps_lock" },
                    "to": [{ "key_code": "left_command" }]
                }
            ],
            "virtual_hid_keyboard": {
                "country_code": 0,
                "keyboard_type_v2": "ansi"
            }
        }
    ]
}
